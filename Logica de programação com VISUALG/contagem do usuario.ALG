Algoritmo "Contador usuario"

Var
n_inicio, n_final: inteiro


Inicio
escreval("Digite o valor inicial")
leia(n_inicio)

escreval("")

escreval("Digite o valor que irá parar a contagem")
leia(n_final)

escreval("")

enquanto n_inicio <= n_final faça
          escreval(n_inicio)
          n_inicio := n_inicio + 1
fimenquanto


Fimalgoritmo